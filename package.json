{
  "name": "@flizpay-de/banks",
  "private": false,
  "version": "0.0.6",
  "type": "module",
  "files": [
    "dist",
    "lib/schemas/*.json"
  ],
  "main": "./dist/@flizpay-de/banks.umd.cjs",
  "module": "./dist/@flizpay-de/banks.js",
  "types": "./dist/banks.d.ts",
  "exports": {
    ".": {
      "import": "./dist/@flizpay-de/banks.js",
